{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Peter Rhodes Portfolio",
  "main": "main.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "4.4.6"
  },
  "scripts": {
    "postinstall": "node nightwatch.conf.js",
    "test": "./node_modules/.bin/nightwatch --env local",
    "ie": "./node_modules/.bin/nightwatch -e ie11",
    "e2e": "npm run sauce; npm run upload",
    "dev": "./node_modules/.bin/nodemon -w test/index.html --delay 250ms --exec 'npm run upload'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RhodesPeter/cv-portfolio.git"
  },
  "author": "Peter Rhodes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RhodesPeter/cv-portfolio/issues"
  },
  "homepage": "https://github.com/RhodesPeter/cv-portfolio#readme",
  "dependencies": {
    "nightwatch": "^0.9.9",
    "saucelabs": "^1.3.0",
    "selenium-download": "^2.0.7"
  },
  "devDependencies": {
    "aws-sdk": "^2.7.13",
    "env2": "^2.1.1"
  }
}
